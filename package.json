{
  "name": "create-bv-pages",
  "version": "1.0.0",
  "type": "module",
  "description": "Create a new project combination of bun, vite for cloudflare pages project",
  "main": "index.ts",
  "scripts": {
    "dev": "bun run --bun vite",
    "build": "vite build && vite build --ssr",
    "preview": "wrangler pages dev",
    "upload": "wrangler pages deploy --branch preview",
    "clean": "rm -rf node_modules && bun --prefer-offline install"
  },
  "dependencies": {},
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240620.0",
    "bun-types": "^1.1.20",
    "vite": "5.3.2",
    "wrangler": "3.62.0"
  },
  "overrides": {
    "esbuild": "0.21.5"
  },
  "patchedDependencies": {
    "wrangler@3.62.0": "patches/wrangler@3.62.0.patch"
  }
}